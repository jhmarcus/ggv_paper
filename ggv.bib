	Journal = {Human Molecular Genetics},
	Keywords = {Europe, Chromosomes: Human, Polymorphism: Single Nucleotide, European Continental Ancestry Group, Genetic Markers, Genome: Human, Homozygote, Genetic Variation, Humans, Heterozygote, Linkage Disequilibrium, Geography},
