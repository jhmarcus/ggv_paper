	Doi = {10.1016/j.ajhg.2009.04.015},
	Journal = {Am J Hum Genet},
	Keywords = {Continental Population Groups, Polymorphism: Single Nucleotide, Computer Simulation, Genetic Markers, Genome: Human, Genetics: Population, Humans, Principal Component Analysis, Pattern Recognition: Automated},
	Language = {eng},
	Month = {May},
