	Mesh = {Algorithms; Alleles; Cluster Analysis; Computational Biology; Computer Graphics; Gene Frequency; Genetics, Population; Genome; Genotype; Humans; Internet; Markov Chains; Monte Carlo Method; Software; User-Computer Interface},
	Number = {1},
	Pmid = {20134055},
