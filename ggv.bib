	Month = {Feb},
	Number = {2},
	Pages = {120--6},
	Pii = {hdy200889},
	Pmid = {18766200},
	Rating = {0},
	Title = {{How accurate is the current picture of human genetic variation?}},
	Uri = {papers://94FA299F-9581-4DF3-9FFC-F9C74C304A21/Paper/p603},
