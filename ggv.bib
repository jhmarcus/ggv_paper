	Pages = {95--108},
	Pii = {AHG489},
	Pmid = {19040659},
	Rating = {0},
	Title = {{Large allele frequency differences between human continental groups are more likely to have occurred by drift during range expansions than by selection}},
	Uri = {papers://94FA299F-9581-4DF3-9FFC-F9C74C304A21/Paper/p599},
