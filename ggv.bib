	Rating = {0},
	Title = {{Formulating a historical and demographic model of recent human evolution based on resequencing data from noncoding regions}},
	Uri = {papers://94FA299F-9581-4DF3-9FFC-F9C74C304A21/Paper/p2412},
	Volume = {5},
	Year = {2010},
	Bdsk-Url-1 = {http://dx.doi.org/10.1371/journal.pone.0010284}}

