	Journal = {PLoS Genetics},
	Keywords = {African Americans, Software, Chromosomes: Human, Evolution: Molecular, Models: Genetic, Genetics: Population, African Continental Ancestry Group, Humans, Disease},
	Language = {eng},
	Number = {6},
	Pages = {e1000519},
	Pmid = {19543370},
	Rating = {0},
	Title = {{Sensitive detection of chromosomal segments of distinct ancestry in admixed populations}},
