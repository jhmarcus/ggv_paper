	Doi = {10.1111/j.1469-1809.2009.00556.x},
	Journal = {Ann Hum Genet},
	Keywords = {European Continental Ancestry Group, Male, Chromosomes, Gene Pool, Chromosomes: Human: Y, Argentina, Genetics: Population, African Continental Ancestry Group, Hispanic Americans, Indians: North American, Humans, DNA: Mitochondrial},
