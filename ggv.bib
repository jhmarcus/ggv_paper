	Mesh = {Cell Line; Continental Population Groups; Genetics, Population; Genome, Human; Genotype; Humans; Linkage Disequilibrium; Polymorphism, Single Nucleotide; Sequence Analysis, DNA},
	Pages = {338},
	Pmc = {PMC2723139},
	Pmid = {19638193},
