	Journal = {Hum Genet},
	Keywords = {, , , , , , , , , , , , , ,},
	Month = {Oct},
	Number = {3},
	Pages = {207--14},
	Pmid = {18752003},
	Rating = {0},
	Title = {{Genome-wide distribution of ancestry in {M}exican Americans}},
	Uri = {papers://94FA299F-9581-4DF3-9FFC-F9C74C304A21/Paper/p1035},
