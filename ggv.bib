@article{Futschik:2010kx,
	Annote = {10.1534/genetics.110.114397},
	Author = {Futschik, Andreas and Schl{\"o}tterer, Christian},
	Date-Added = {2012-02-06 01:18:47 -0800},
	Date-Modified = {2012-02-06 01:18:47 -0800},
	Journal = {Genetics},
	Journal1 = {Genetics},
	Number = {1},
	Pages = {207--218},
	Title = {The Next Generation of Molecular Markers From Massively Parallel Sequencing of Pooled DNA Samples},
	Ty = {JOUR},
	Url = {http://www.genetics.org/content/186/1/207.abstract N2 - Next generation sequencing (NGS) is about to revolutionize genetic analysis. Currently NGS techniques are mainly used to sequence individual genomes. Due to the high sequence coverage required, the costs for population-scale analyses are still too high to allow an extension to nonmodel organisms. Here, we show that NGS of pools of individuals is often more effective in SNP discovery and provides more accurate allele frequency estimates, even when taking sequencing errors into account. We modify the population genetic estimators Tajima's π and Watterson's θ to obtain unbiased estimates from NGS pooling data. Given the same sequencing effort, the resulting estimators often show a better performance than those obtained from individual sequencing. Although our analysis also shows that NGS of pools of individuals will not be preferable under all circumstances, it provides a cost-effective approach to estimate allele frequencies on a genome-wide scale.},
	Volume = {186},
	Year = {September 2010},
	Bdsk-Url-1 = {http://www.genetics.org/content/186/1/207.abstract%20N2%20-%20Next%20generation%20sequencing%20(NGS)%20is%20about%20to%20revolutionize%20genetic%20analysis.%20Currently%20NGS%20techniques%20are%20mainly%20used%20to%20sequence%20individual%20genomes.%20Due%20to%20the%20high%20sequence%20coverage%20required,%20the%20costs%20for%20population-scale%20analyses%20are%20still%20too%20high%20to%20allow%20an%20extension%20to%20nonmodel%20organisms.%20Here,%20we%20show%20that%20NGS%20of%20pools%20of%20individuals%20is%20often%20more%20effective%20in%20SNP%20discovery%20and%20provides%20more%20accurate%20allele%20frequency%20estimates,%20even%20when%20taking%20sequencing%20errors%20into%20account.%20We%20modify%20the%20population%20genetic%20estimators%20Tajima's%20%CF%80%20and%20Watterson's%20%CE%B8%20to%20obtain%20unbiased%20estimates%20from%20NGS%20pooling%20data.%20Given%20the%20same%20sequencing%20effort,%20the%20resulting%20estimators%20often%20show%20a%20better%20performance%20than%20those%20obtained%20from%20individual%20sequencing.%20Although%20our%20analysis%20also%20shows%20that%20NGS%20of%20pools%20of%20individuals%20will%20not%20be%20preferable%20under%20all%20circumstances,%20it%20provides%20a%20cost-effective%20approach%20to%20estimate%20allele%20frequencies%20on%20a%20genome-wide%20scale.}}

@article{Kofler23102011,
	Abstract = {Summary: Sequencing pooled DNA samples (Pool-Seq) is the most cost-effective approach for the genome-wide comparison of population samples. Here, we introduce PoPoolation2, the first software tool specifically designed for the comparison of populations with Pool-Seq data. PoPoolation2 implements a range of commonly used measures of differentiation (FST, Fisher's exact test and Cochran-Mantel-Haenszel test) that can be applied on different scales (windows, genes, exons, SNPs). The result may be visualized with the widely used Integrated Genomics Viewer.Availability and implementation: PoPoolation2 is implemented in Perl and R. It is freely available on http://code.google.com/p/popoolation2/Contact: christian.schloetterer@vetmeduni.ac.atSupplementary information: Manual: http://code.google.com/p/popoolation2/wiki/Manual Test data and tutorial: http://code.google.com/p/popoolation2/wiki/Tutorial Validation: http://code.google.com/p/popoolation2/wiki/Validation},
	Author = {Kofler, Robert and Pandey, Ram Vinay and Schl{\"o}tterer, Christian},
	Date-Added = {2012-02-06 01:18:04 -0800},
	Date-Modified = {2012-02-06 01:18:04 -0800},
	Eprint = {http://bioinformatics.oxfordjournals.org/content/early/2011/10/23/bioinformatics.btr589.full.pdf+html},
	Journal = {Bioinformatics},
	Title = {PoPoolation2: Identifying differentiation between populations using sequencing of pooled DNA samples (Pool-Seq)},
