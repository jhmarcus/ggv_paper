	Keywords = {Selection: Genetic, Oceanic Ancestry Group, Computer Simulation, Evolution: Molecular, Genetic Variation, Linkage Disequilibrium, Polymorphism: Single Nucleotide, Gene Flow, Models: Genetic, Haplotypes, Tonga, Genotype, Genetics: Population, Humans, Papua New Guinea},
